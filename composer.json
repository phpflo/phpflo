{
  "name": "phpflo/phpflo-component",
  "description": "Collection of base components for usage with phpflo",
  "type": "library",
  "keywords": ["fbp", "flow"],
  "license": "MIT",
  "authors": [
    {
      "name": "Henri Bergius",
      "email": "henri.bergius@iki.fi",
      "homepage": "http://bergie.iki.fi/"
    },
    {
      "name": "Marc Aschmann",
      "email": "maschmann@gmail.com"
    }
  ],
  "require": { "php": ">=7.0.0" },
  "require-dev": {
    "phpflo/phpflo": "^2.0.0",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "PhpFlo\\": "lib/PhpFlo/",
      "Test\\": "lib/Test/"
    }
  },
  "config": { "bin-dir": "bin" },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  }
}
